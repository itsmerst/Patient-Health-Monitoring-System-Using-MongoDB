    <script>document.getElementById("prescription-form").addEventListener("submit", function(event) {
        event.preventDefault(); // Prevent form submission
      
        // Get the selected patient name from the dropdown
        var selectedPatient = document.getElementById("patient-name-input").value;
      
        // Make a GET request to fetch the prescription data for the selected patient
        fetch(`/api/registrationForm?patient=${selectedPatient}&timestamp=${Date.now()}`)
          .then(function(response) {
            if (response.ok) {
              return response.json();
            } else {
              throw new Error('Error fetching prescription data');
            }
          })
          .then(function(data) {
            // Display the prescription data in the readonly input field
            document.getElementById("prescription-info").value = data.prescription;
          })
          .catch(function(error) {
            // Error occurred while making the request or handling the response
            console.log("Error:", error);
          });
      });
      </script>

